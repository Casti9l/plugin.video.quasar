<window>
	<!-- 1280 x 720 -->
	<defaultcontrol always="false">32503</defaultcontrol>
    <allowoverlay>no</allowoverlay>
	<animation effect="slide" start="0,650" time="200">WindowOpen</animation>
	<animation effect="slide" end="0,650" time="200">WindowClose</animation>
    <controls>
		<control type="group">
			<control type="image">
				<!-- Background -->
				<posx>190</posx>
				<posy>60</posy>
				<width>900</width>
				<height>600</height>
				<texture>AddonWindow/ContentPanel.png</texture>
				<!--<colordiffuse>EEFFFFFF</colordiffuse>-->
			</control>
			<control type="image">
				<posx>340</posx>
				<posy>69</posy>
				<width>600</width>
				<height>35</height>
				<texture>AddonWindow/dialogheader.png</texture>
			</control>
			<control type="button" id="32500">
				<!-- Close button -->
				<posx>1020</posx>
				<posy>67</posy>
				<width>60</width>
				<height>30</height>
				<texturefocus>AddonWindow/DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>AddonWindow/DialogCloseButton.png</texturenofocus>
				<onup>32508</onup>
				<ondown>32503</ondown>
			</control>
			<control type="label" id="32501">
				<!-- Window title -->
				<posx>290</posx>
				<posy>72</posy>
				<width>700</width>
				<height>30</height>
				<font>font13</font>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>FFFFA500</textcolor>
				<shadowcolor>AA000000</shadowcolor>
			</control>
			<control type="scrollbar" id="32502">
				<!-- Scrollbar -->
				<posx>1037</posx>
				<posy>142</posy>
				<width>25</width>
				<height>470</height>
				<onleft>32503</onleft>
				<onup>32500</onup>
				<texturesliderbackground border="0,14,0,14">Scrollbar/ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="2,16,2,16">Scrollbar/ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="2,16,2,16">Scrollbar/ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>Scrollbar/ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>Scrollbar/ScrollBarNib.png</textureslidernibfocus>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>

			<control type="panel" id="32503">
				<!-- Panel -->
				<posx>246</posx>
				<posy>150</posy>
				<width>760</width>
				<height>455</height>
				<onleft>2</onleft>
				<onright>32502</onright>
				<onup>32500</onup>
				<align>center</align>
				<viewtype>list</viewtype>
				<focusposition>0</focusposition>
				<pagecontrol>32502</pagecontrol>
				<scrolltime>200</scrolltime>
				<preloaditems>2</preloaditems>
				<visible>IsEmpty(Window(home).Property(loading))</visible>
				<itemlayout height="65" width="750">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>750</width>
						<height>60</height>
						<texture border="5">Button/button-nofocus.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>2</posy>
						<width>727</width>
						<height>30</height>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>silver</textcolor>
						<shadowcolor>AA000000</shadowcolor>
						<info>ListItem.Label</info>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>28</posy>
						<width>727</width>
						<height>30</height>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>silver</textcolor>
						<shadowcolor>AA000000</shadowcolor>
						<info>ListItem.Label2</info>
					</control>
				</itemlayout>

				<focusedlayout height="65" width="750">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>750</width>
						<height>60</height>
						<texture border="5">Button/Button-focus.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>2</posy>
						<width>727</width>
						<height>30</height>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<shadowcolor>AA000000</shadowcolor>
						<info>ListItem.Label</info>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>28</posy>
						<width>727</width>
						<height>30</height>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<shadowcolor>AA000000</shadowcolor>
						<info>ListItem.Label2</info>
					</control>
				</focusedlayout>
			</control>
		</control>
    </controls>
</window>
